<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.letsun.xms.mapper.scan.UnlawfulScanMapper" >
	
<resultMap id="BaseResultMap" type="com.test.model.Record">
  <id column="id" property="id" jdbcType="BIGINT" />
  <result column="openid" property="openid" jdbcType="VARCHAR" />
  <result column="nick_name" property="nickName" jdbcType="VARCHAR" />
  <association property="member" column="openid" javaType="com.test.model.Member" select="selectMember"/>              
</resultMap>

 <select id="selectMember" parameterType="java.lang.String" resultMap="com.test.mapper.MemberMapper.BaseResultMap">
 		select * from member t where t.openid = #{openid}
 </select>
 
